{
  "name": "full-stack-coding-challenge",
  "version": "0.1.0",
  "repository": "git@github.com:tilla-tech/full-stack-coding-challenge-backend.git",
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "resolutions": {
    "typescript": "4.8.3",
    "ts-node": "^10.0.0"
  },
  "dependencies": {
    "concurrently": "^7.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "concurrently \"yarn --cwd ./frontend dev\" \"yarn --cwd ./backend start:dev\"",
    "prisma:generate": "prisma generate --schema=./backend/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./backend/prisma/schema.prisma",
    "docker:up": "docker-compose -f docker-compose.dev.yaml up  --abort-on-container-exit"
  }
}
